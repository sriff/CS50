#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int h;
    do
    {
        h = get_int("Height: ");
    }
    while (h > 0 && h < 9);
    printf("Stored: %i\n", h);
    for (int i = 0; i < h; i++)
    {
        for (int j = 0; j< h; j++)
    
        {
            printf("#");
        }
        printf("\n");
    }  
}
